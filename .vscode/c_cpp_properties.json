{
    "configurations": [
        {
            "name": "Embedded",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/**",
                "${workspaceFolder}/.vscode/",
                "${workspaceFolder}/lib/**",
                "${workspaceFolder}/lib/Chimera",
                "${workspaceFolder}/lib/FreeRTOS",
                "${workspaceFolder}/lib/FreeRTOS/portable/GCC/ARM_CM4F",
                "${workspaceFolder}/lib/Thor",
                "${workspaceFolder}/lib/Thor/Thor/config/chimera",
                "${workspaceFolder}/lib/Thor/Thor/config/freertos",
                "${BOOST_ROOT}/boost_1_70_0"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "CHIMERA_LITTLE_ENDIAN",
                "USING_FREERTOS",
                "STM32F446xx",
                "BOOST_NO_EXCEPTIONS",
                "_EMBEDDED",
                "USE_FREERTOS",
                "THOR_CUSTOM_DRIVERS=1",
                "TARGET_STM32F4"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17"
        },
        {
            "name": "Simulator",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/**",
                "${workspaceFolder}/.vscode/",
                "${workspaceFolder}/lib/**",
                "${workspaceFolder}/lib/Chimera",
                "${workspaceFolder}/lib/FreeRTOS",
                "${workspaceFolder}/lib/FreeRTOS/portable/GCC/ARM_CM4F",
                "${workspaceFolder}/lib/Thor",
                "${workspaceFolder}/lib/Thor/Thor/config/chimera",
                "${workspaceFolder}/lib/Thor/Thor/config/freertos",
                "${BOOST_ROOT}/boost_1_70_0"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "CHIMERA_LITTLE_ENDIAN",
                "USING_FREERTOS",
                "STM32F446xx",
                "BOOST_NO_EXCEPTIONS",
                "_EMBEDDED",
                "USE_FREERTOS",
                "THOR_CUSTOM_DRIVERS=1",
                "RF24_SIMULATOR"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}